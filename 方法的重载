public class ChongzaiChongxie {
    public static void main(String[] args){
        User14 user1 = new User14();
        user1.login("lsdk","ldk");
        user1.login(12334);
        user1.login("lkdjl");
    }
}
class User14{
    public User14() {
        System.out.println("父类空参构造器");
    }
    void login(String account, String password){
        System.out.println("账号密码登陆");
    }
    void login(int tel){
        System.out.println("手机号登陆");
    }
    void login(String wx){
        System.out.println("微信登陆，支付宝登陆");
    }
}

